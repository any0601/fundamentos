#include <iostream>

using namespace std; 

int main()
{
    int b,h,r;
    cout<<"base del paralelogramo: ";
    cin>> b;
    cout<<"escribir altura del paralelogramo: ";
    cin>> h; 
    r=b*h; 
    cout<<"es: "<<r;
    return 0;
}
